		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"mdsvex": "^0.12.5",
		"mdsvex-relative-images": "^2.0.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
